{
  "manifest_version": 3,
  "name": "Thời khóa biểu SGU",
  "version": "1.0",
  "description": "Thêm chức năng xem thời khóa biểu",

  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["http://thongtindaotao.sgu.edu.vn/*"]
    }
  ],
  "action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "64": "images/64x64.png"
  }
}
